package assigment1;
public class main {
    public static void main(String[] args) {
        String hoVaTen = "Cao Tan Thanh";
        String lop = "12C3";
        String maSoSinhVien = "123456";

        System.out.println("Họ và tên: " + hoVaTen);
        System.out.println("Lớp: " + lop);
        System.out.println("Mã số sinh viên: " + maSoSinhVien);
    }
}   
	
